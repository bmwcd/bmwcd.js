{
  "name": "bmwcd.js",
  "version": "0.1.6",
  "description": "BMW ConnectedDrive module for Node",
  "author": "Nicholas Berlette",
  "homepage": "https://bmwcd.github.io/bmwcd-js",
  "type": "module",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmwcd/bmwcd.js.git"
  },
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel ./src --out-dir ./dist",
    "start": "cross-env BABEL_ENV=development babel --watch ./src --out-dir ./dist",
    "test": "node -r dotenv/config ./examples/basic.js",
    "test:fs": "node -r dotenv/config ./examples/fs.js",
    "test:maps": "node -r dotenv/config ./examples/maps.js",
    "lint": "standard && standard-markdown",
    "lint:fix": "standard --fix && standard-markdown --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "await-to-js": "^2.1.1",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "js-yaml": "^3.14.1",
    "moment": "^2.29.1",
    "qs": "^6.9.1",
    "toker.js": "^0.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "prettier": "^2.3.0",
    "cross-env": "^7.0.3",
    "standard": "^16.0.3",
    "standard-markdown": "^6.0.0"
  }
}
